unit CondicoesDTO;

interface
  type
    TCondicoesDTO = class
      private
        FCodigo: string;
        FDescricao: string;
      public
       property Codigo : String read FCodigo write FCodigo;
        property Descricao : String read FDescricao write FDescricao;
       destructor destroy;override;
    end;

implementation

{ TCondicoesDTO }

destructor TCondicoesDTO.destroy;
begin

  inherited;
end;

end.

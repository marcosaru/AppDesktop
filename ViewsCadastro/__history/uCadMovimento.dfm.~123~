  object QryVenda: TFDQuery
    Connection = fdmConexao.BANCO
    SQL.Strings = (
      'select mov_codigo,cli_nome,mov_data,mov_tipo,'
      'mov_totalProdutos,bai_nome,mun_nome,Cli_endereco,mov_entrega,'
      'mov_totalservicos,cto_valor,'
      'mov_acrescimo,mov_desconto,         '
      'mov_frete,mov_qtdeitens'
      ',mov_valorfinal,mov_observacoes,con_descricao,'
      'forpagto_descricao,mov_obspagamento'
      'from movimento '
      'join cliente on cli_codigo = mov_cliente'
      'join bairro on bai_codigo = cli_bairro'
      'join municipio on mun_codigo = cli_municipio'
      'join contato on cto_cliente = cli_codigo'
      'join formapagto on forpagto_codigo = mov_forpagto'
      'join condicoes on con_codigo = mov_condicoes'
      'where 1=1 and cto_tipo = 1')
    Left = 769

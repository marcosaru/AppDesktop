  object QryItens: TFDQuery
    Connection = fdmConexao.BANCO
    SQL.Strings = (
      'select mitens_movimento,mitens_quantidade,mitens_produto,'
      'pro_nome,pro_valorvenda,pro_medidas,pro_tecido '
      'from movimentoitens'
      'Join produto on pro_codigo = mitens_produto')
